{
    "name": "Chrome Extension with React & Webpack",
    "version": "1.0.0",
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "icons/icon-34.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "128": "icons/icon-128.png"
    },
    "web_accessible_resources": [
        "icons/icon-128.png",
        "icons/icon-34.png"
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>",
        "*://*/*",
        "*://youta-api.ngrok.io/*",
        "*://youta-api.ngrok.io//*",
        "http://youta-api.ngrok.io/*",
        "http://youta-api.ngrok.io//*",
        "https://youta-api.ngrok.io/*",
        "https://youta-api.ngrok.io//*"
    ],
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
